    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "fs": "0.0.1-security"
  }

    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.9",
